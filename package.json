{
  "name": "git-extract",
  "version": "0.1.0",
  "description": "Extract files or folders from a Git repository.",
  "author": "Philip La <birkal@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CreaturePhil/git-extract/issues"
  },
  "scripts": {
    "test": "grunt && mocha --reporter=spec --timeout=10000",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec --timeout=10000 && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "keywords": [
    "extract",
    "extraction",
    "file",
    "folder",
    "git",
    "github",
    "clone",
    "repo",
    "repository"
  ],
  "main": "bin/extract",
  "preferGlobal": true,
  "bin": {
    "extract": "./bin/extract"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CreaturePhil/git-extract.git"
  },
  "homepage": "https://github.com/CreaturePhil/git-extract",
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {
    "chalk": "^1.0.0",
    "commander": "^2.7.1",
    "fs-extra": "^0.16.5"
  },
  "devDependencies": {
    "chai": "^2.1.1",
    "coveralls": "^2.11.2",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-jshint": "^0.11.0",
    "istanbul": "^0.3.7",
    "mocha": "^2.2.1",
    "mocha-lcov-reporter": "0.0.2"
  }
}
