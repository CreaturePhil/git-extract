{
  "name": "git-extract",
  "version": "0.1.0",
  "description": "Extract certain files or folders from a git repository on Github.",
  "author": "Philip La <birkal@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CreaturePhil/git-extract/issues"
  },
  "scripts": {
